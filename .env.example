# Environment Configuration Example
# Copy this file to .env and fill in your values

# Team B API (fill this after deploying to RunPod)
TEAM_B_API_URL=https://xxxxx-8000.proxy.runpod.net
TEAM_B_NOVNC_URL=https://xxxxx-6080.proxy.runpod.net

# CARLA Configuration
CARLA_HOST=localhost
CARLA_PORT=2000
CARLA_TIMEOUT=10.0

# YOLO Configuration
YOLO_MODEL=yolov8n.pt
YOLO_DEVICE=cuda
YOLO_CONFIDENCE=0.5

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Development
DEBUG=false
LOG_LEVEL=INFO
